<% include ../partials/_head %>
	<table>
		<% for(let index in allTitles) {%>
			<tr> 
				<td> <%= allTitles[index]%> </td>
				<td>
					<a href="/api/titles/edit/<%= allTitles[index]._id %>"> Edit Title </a>
				</td>
			</tr>
		<%}%>
	</table>

	<form action="/api/titles" method="POST">
		<h3> Create New Title </h3>
		<input type="text" name="name" placeholder="Name" required/> <br/>
		<button type="submit">Create</button>
    </form>

<% include ../partials/_footer %>
